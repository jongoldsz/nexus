<h1>Listing projects</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Status</th>
    <th>Pitch</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @projects.each do |project| %>
  <tr>
    <td><%= project.name %></td>
    <td><%= project.status %></td>
    <td><%= project.pitch %></td>
    <td><%= project.description %></td>
    <td><%= link_to 'Show', project %></td>
    <td><%= link_to 'Edit', edit_project_path(project) %></td>
    <td></td>
  </tr>
<% end %>
</table>

