<% @user.projects.each do |project| %>
  <div class="user_project_involvement" id="user_project_<%= project.id %>">
  <%= project.name %>
  </div>
<% end %>

<% @user.memberships.each do |membership| %>
  <div class="user_project_involvement" id="user_project_<%= membership.project.id %>">
  <%= membership.project.name %>
  </div>
<% end %>
